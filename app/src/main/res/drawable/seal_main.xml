<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="326.5dp"
    android:height="326.5dp"
    android:viewportHeight="326.5"
    android:viewportWidth="326.5">
    <group
        android:name="center"
        android:scaleX="1"
        android:scaleY="1">
        <!--
          _____
         -     -
        |       |
        |       |
         -     -
        六芒星六个顶点
        顶
        肩左
        肩右
        胯左
        胯右
        底
        中心
        中心圆 半径20
        -->

        <!--逆时针主体六芒星-->
        <path
            android:name="start"
            android:pathData=
                "M 163.25 37
                L 272.58 100.12
                L 272.58 226.3
                L 163.25 289.5
                L 53.91  226.37
                L 53.91 100.12
                L 163.25 37 Z"
            android:strokeColor="#eeeeee"
            android:strokeWidth="1.5" />

        <!--逆时针正三角
        顶点预留线宽距离
        顶点y-3
        底部两点x内缩2
        -->
        <path
            android:name="positive_triangle"
            android:pathData="M 163.25 40
            L 55.91  226.37
            L 270.58 226.3 Z"
            android:strokeColor="#eeeeee"
            android:strokeWidth="4" />

        <!--顺时针反三角-->
        <path
            android:name="negative_triangle"
            android:pathData=" M 163.25 286.5
            L 55.91 100.12
            L 270.58 100.12 Z"
            android:strokeColor="#eeeeee"
            android:strokeWidth="4"
            />

        <group
            android:name="lines">
            <!--连接中心点的线组合-->
            <path android:name="line_1"
                android:pathData="M 163.25 37 L 163.25 163.25"
                android:strokeColor="#eeeeee"
                android:strokeWidth="1"
                />
            <path android:name="line_2"
                android:pathData="M 272.58 100.12 L 163.25 163.25"
                android:strokeColor="#eeeeee"
                android:strokeWidth="1"
                />
            <path android:name="line_3"
                android:pathData="M 272.58 226.3 L 163.25 163.25"
                android:strokeColor="#eeeeee"
                android:strokeWidth="1"
                />
            <path android:name="line_4"
                android:pathData="M 163.25 289.5 L 163.25 163.25"
                android:strokeColor="#eeeeee"
                android:strokeWidth="1"
                />
            <path android:name="line_5"
                android:pathData="M 53.91  226.37 L 163.25 163.25"
                android:strokeColor="#eeeeee"
                android:strokeWidth="1"
                />
            <path android:name="line_6"
                android:pathData="M 53.91 100.12 L 163.25 163.25"
                android:strokeColor="#eeeeee"
                android:strokeWidth="1"
                />
        </group>

        <!--inner little circle-->
        <path
            android:name="start_circle"
            android:pathData=" M 143.25 163.25
                    A 20 20 0 0 0 183.25 163.25
                    A 20 20 0 0 0 143.25 163.25"
            android:strokeColor="#eeeeee"
            android:strokeWidth="1" />
    </group>


</vector>